var array= [12,23,33,23,11,223];
alert (biggest(array));
function biggest(array){
var bigger=0;
for(i=0;i<array.length;i++){if(array[i]>bigger){bigger=array[i]}}
return bigger;
}

// another one of the same
function greatest(array){
    var a=array.length;
    
    var c=0;
    for(d=0;d<array.length;d++){
    if(array[d]>c){c=array[d];}
    }
    return c;};
    console.log(greatest([1,2,3,4,5,6,7,8,9]));
